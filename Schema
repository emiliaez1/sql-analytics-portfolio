CREATE TABLE users (
    user_id INT PRIMARY KEY,
    signup_date DATE,
    country VARCHAR(50)
);

CREATE TABLE user_activity (
    user_id INT,
    activity_date DATE
);

CREATE TABLE campaigns (
    campaign_id INT PRIMARY KEY,
    campaign_name VARCHAR(100)
);

CREATE TABLE campaign_events (
    campaign_id INT,
    event_date DATE,
    clicks INT,
    impressions INT
);
